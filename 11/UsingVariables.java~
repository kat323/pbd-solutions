public class UsingVariables {
    public static void main (String[] args) {
        int room;
        double e;
        String subject;

        room = 215;
        e = 216;
        subject = "something";

        System.out.println("This is room # " + room);
        System.out.println("e is close to " + e);
        System.out.println("I am learning a bit about " + subject);
    }
}
