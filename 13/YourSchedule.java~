public class YourSchedule {
    public static void main (String[] args) {
        String course1 = "Economy", course2 = "Canadian Law", course3 = "Computer Science", course4 = "Spare", teacher1 = "Ms. D'Elia", teacher2 = "Mr. Sirianni", teacher3 = "Mr. Gallo", teacher4 = "N/A";
        
        System.out.println("+--------------------------------+");        
        System.out.println("|1|            " + course1 + "|" + teacher1 + "|"); 
        System.out.println("|2|     " + course2 + "|" + teacher2 + "|"); 
        System.out.println("|3|    " + course3 + "|" + teacher3 + "|"); 
        System.out.println("|4|                     " + course4 + "|" + teacher4 + "|"); 
        System.out.println("+--------------------------------+"); 
    }
}
